<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.databasemysql.loginpage">

<padding>
    <Insets bottom="20" left="20" right="20" top="20" />
</padding>
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="302.0" text="About" GridPane.columnIndex="3" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets />
      </GridPane.margin></Button>

<Label prefHeight="17.0" prefWidth="104.0" text="Username:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
<TextField fx:id="usernameField" prefHeight="25.0" prefWidth="305.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />

<Label text="Password:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
<PasswordField fx:id="passwordField" prefHeight="25.0" prefWidth="306.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />

<Label text="Address:" GridPane.columnIndex="1" GridPane.rowIndex="3" />
<TextField fx:id="addressField" GridPane.columnIndex="2" GridPane.rowIndex="3" />

<Button onAction="#connect" prefHeight="25.0" prefWidth="86.0" text="Connect" GridPane.columnIndex="2" GridPane.rowIndex="4" />
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="265.0" text="Licence" GridPane.columnIndex="3" GridPane.rowIndex="2" />
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="274.0" text="Source" GridPane.columnIndex="3" GridPane.rowIndex="3" />
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="300.0" text="Donate" GridPane.columnIndex="3" GridPane.rowIndex="4" />
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" prefWidth="200.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
